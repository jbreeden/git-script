    <panel>
      <chart>
        <title>This Year of Commits</title>
        <search>
          <query>index="git" kind=*commit earliest=-365d | bucket _time span=1d | rename _time as "Time" | chart count AS "Commit Count" by "Time"</query>
          <earliest>0</earliest>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
