    <panel>
      <chart>
        <title>Committers Per Year</title>
        <search>
          <query>index=git kind=*commit | fields author_email author_name author_date | eval year=strftime(strptime('author_date', "%Y-%m-%d %H:%M:%S %Z"), "%Y") | eval name_email_year='author_name'.'author_email'.'year' | dedup name_email_year | stats count by year</query>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
